class Person {
    String name;
    Person(String name) {
        this.name = name;
    }
}

public class Main {
    static void changePerson(Person p) {
        p.name = "Bob"; 
    }

    public static void main(String[] args) {
        Person person = new Person("Maga");
        changePerson(person);
        System.out.println(person.name);
    }
}
