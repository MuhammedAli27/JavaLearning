public class Zero {
    public static void main(String[] args) {
        try {
            int a = 10 / 0;
        } catch (ArithmeticException e) {
            System.out.println("Ошибка: деление на ноль!);
        } finally {
            System.oyt.println("Этот блок вечный!!!");
        }
    }
}
